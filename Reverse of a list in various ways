# Using insert() method
l = [0,1,2,3,4]
rl = []
for i in l:
  rl.insert(0,i)
print("Reverse of elements:",rl )


# using user defined method
def rev(l):
  return [i for i in reversed(l)]

l = [4,5,6]
rev(l)  

# using indexing method
m = [1,2,3,4]
m[::-1]

# using predefined function
u = [22,33,44]
u.reverse()
u
